    <Nullable>enable</Nullable>
    <AssemblyName>LunaHost</AssemblyName>
    <RootNamespace>$(LunaHost.Replace(" ", "_"))</RootNamespace>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Swegger\dist\index.html" />
